{
  "expo": {
    "name": "Sulprev Shop",
    "slug": "sulprev-shop",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/iconFront.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/iconHome.png",
      "resizeMode": "stretch",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.forghieri.sulprevshop",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/iconFront.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.forghieri.sulprevshop"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "021bdadf-7346-4ca1-9c51-1971437b84d8"
      }
    },
    "owner": "forghieri",
    "plugins": [
      "expo-sqlite"
    ]
  }
}
